{
    "name": "jharkhand-civic-reporting",
    "version": "1.0.0",
    "private": true,
    "description": "Government of Jharkhand Civic Issue Reporting Platform",
    "keywords": [
        "government",
        "civic",
        "reporting",
        "jharkhand",
        "nextjs",
        "react",
        "typescript",
        "postgresql",
        "mui",
        "prisma",
        "tanstack"
    ],
    "author": "Government of Jharkhand",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "prisma generate --schema=./src/prisma/schema.prisma && next build",
        "start": "next start",
        "lint": "next lint",
        "postinstall": "prisma generate --schema=./src/prisma/schema.prisma",
        "db:seed": "cd src && npx ts-node prisma/seed.ts",
        "db:seed-complaints": "cd src && npx ts-node scripts/seedDummyComplaints.ts",
        "db:push": "npx prisma db push --schema=./src/prisma/schema.prisma",
        "db:migrate": "npx prisma migrate dev --schema=./src/prisma/schema.prisma"
    },
    "dependencies": {
        "@emoji-mart/data": "^1.1.2",
        "@emoji-mart/react": "^1.1.1",
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^7.3.2",
        "@mui/material": "^5.13.1",
        "@prisma/client": "^5.7.1",
        "@tanstack/react-query": "^4.29.7",
        "@types/leaflet": "^1.9.20",
        "@types/node": "^18.16.13",
        "@types/pg": "^8.15.5",
        "@types/react": "^18.2.6",
        "@types/react-dom": "^18.2.4",
        "bcrypt": "^5.1.0",
        "date-fns": "^2.30.0",
        "emoji-mart": "^5.5.2",
        "eslint": "8.38.0",
        "eslint-config-next": "^14.2.5",
        "formik": "^2.2.9",
        "framer-motion": "^10.12.12",
        "jose": "^4.14.4",
        "leaflet": "^1.9.4",
        "next": "^14.2.5",
        "pg": "^8.16.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "react-icons": "^4.8.0",
        "react-infinite-scroll-component": "^6.1.0",
        "react-leaflet": "^4.2.1",
        "universal-cookie": "^4.0.4",
        "yup": "^1.1.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "encoding": "^0.1.13",
        "eslint-config-prettier": "^8.8.0",
        "prettier": "^2.8.8",
        "prisma": "^5.7.1",
        "sass": "^1.62.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
